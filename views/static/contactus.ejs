      <div class="tab-pane fade" id="contact-us-form">        
        <h1 class="text-center">Contact the Maersk Group</h1>
        <br>
        <form role="form" data-toggle="validator" class="form-horizontal">            
          <fieldset>                                
            <div class="form-group">
              <label for="contactName" class="col-lg-3 control-label">Name</label>
              <div class="col-lg-6">
                <input type="text" class="form-control" id="contactName" placeholder="Please enter your contact name *" data-error="  Please fill out this field" required>

              </div>
            </div>
            <div class="form-group">
              <label for="contactEmail" class="col-lg-3 control-label">Email</label>
              <div class="col-lg-6">
                <input type="email" class="form-control" id="contactEmail" placeholder="Please enter your contact email *" data-error="  Please fill out this field" required>  
                <div class="help-block with-errors"></div>                
              </div>
            </div>
            <div class="form-group">
              <label for="contactSubject" class="col-lg-3 control-label">Subject</label>
              <div class="col-lg-6">
                <input type="text" class="form-control" id="contactSubject" placeholder="Please enter your email subject *" data-error="  Please fill out this field" required>
                <div class="help-block with-errors"></div>
              </div>
            </div>                                       
            <div class="form-group">
              <label for="textArea" class="col-lg-3 control-label">Message</label>
              <div class="col-lg-6">
                <textarea class="form-control form-control-white" rows="6" id="textArea" placeholder="Please type your message here *" data-error="  Please fill out this field" required></textarea>
                <div class="help-block with-errors"></div>
                <!-- <span class="help-block"></span> -->
              </div>
            </div>
            <div class="form-group">
              <div class="col-lg-10 col-lg-offset-3">                  
                <button type="submit" class="btn btn-lg btn-primary">Send</button>
              </div>
            </div>
          </fieldset>
        </form>
      </div> <!-- endTab_Contact-form -->
    </div> <!-- endTab-content -->  
