<script>
    function initMap() {
        var myLatLng = {lat: 55.687658, lng: 12.597378}; 

        // Create a map object and specify the DOM element for display.
        var map = new google.maps.Map(document.getElementById('map'), {
          center: myLatLng,
          scrollwheel: false,
          zoom: 17
        });

        // Create a marker and set its position.
        var marker = new google.maps.Marker({
          map: map,
          position: myLatLng,          
        });
      }
  </script>  
  <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyDrUTcye4syRYBH-PLGhPFXJ4zZI6C6UJ0&callback=initMap" async defer></script>